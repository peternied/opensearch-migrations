{
  "default": {
    "stage": "dev",
    "targetCluster": {
      "endpoint": "http://TARGET_CLUSTER_ENDPOINT",
      "auth": {
        "type": "sigv4",
        "// basic auth documentation": "The next two lines are releavant for basic auth only",
        "username": "<USERNAME>",
        "passwordFromSecretArn": "<ARN_OF_SECRET_CONTAINING_PASSWORD>",
        "// sigv4 documentation": "The next two lines are releavant for sigv4 only",
        "region": "us-west-2",
        "serviceSigningName": "es"
      }
    },
    "sourceCluster": {
      "endpoint": "http://SOURCE_CLUSTER_ENDPOINT",
      "auth": {
        "type": "sigv4",
        "// sigv4 documentation": "The next two lines are releavant for sigv4 only",
        "region": "us-west-2",
        "serviceSigningName": "aoss"
      }
    },
    "reindexFromSnapshotServiceEnabled": true,
    "reindexFromSnapshotMaxShardSizeGiB": 80,
    "trafficReplayerServiceEnabled": false
  },
  "availability-zones:account=336984078605:region=us-west-2": [
    "us-west-2a",
    "us-west-2b",
    "us-west-2c",
    "us-west-2d"
  ]
}
